{
  "manifest_version": 3,
  "name": "Tab Position",
  "version": "1.0",
  "description": "Enumerates each tab in the current browser window.",
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
